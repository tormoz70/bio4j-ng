<?xml version="1.0" encoding="UTF-8"?>
<cursor xmlns="http://bio4j.ru/biodef-repo/store"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="d:\jdev\workspace\bio4j-ng\app-server\as-distribution-as\src\main\biodef\store-v10.xsd"
       multiselection="false">
    <SQL action="select">
        <text><![CDATA[
select usrid as sortord, usrid, login, null as passwd, null as passwd1, email, fio, notes, expiredfrom, enabled, isadmin
  from USR
        ]]></text>
    </SQL>
    <SQL action="update"><text>user_edit</text></SQL>
    <fields>
        <field name="usrid" type="integer" pk="true" format="0" header="ID"/>
        <field name="login" type="string" header="Логин" width="100"/>
        <field name="passwd" type="string" hidden="true"/>
        <field name="passwd1" type="string" hidden="true"/>
        <field name="email" type="string" header="Email" width="100"/>
        <field name="fio" type="string" header="ФИО" width="200"/>
        <field name="notes" type="string" header="Примечание" width="200" showTooltip="true"/>
        <field name="expiredfrom" type="date" header="Действителен до" width="100"/>
        <field name="isadmin" type="boolean" header="Админ" width="80"/>
        <field name="enabled" type="boolean" header="Активен" width="80"/>
    </fields>
</cursor>
