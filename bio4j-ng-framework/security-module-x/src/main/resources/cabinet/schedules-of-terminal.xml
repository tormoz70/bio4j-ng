<?xml version="1.0" encoding="UTF-8"?>
<cursor xmlns="http://bio4j.ru/biodef-repo/store"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="d:\jdev\workspace\bio4j-ng\app-server\as-distribution-as\src\main\biodef\store-v10.xsd"
       multiselection="false">
    <SQL action="select">
        <text><![CDATA[
select
  st.aterminalid,
  st.schtermid,
  st.priority,
  st.isactive,
  st.scheduleid,
  s.aname
from SCHTERM st
	inner join SCHEDULE s on s.scheduleid = st.scheduleid and not s.isdeleted
where st.aterminalid = :aterminalid
order by priority
        ]]></text>
        <param name="aterminalid" type="integer"/>
    </SQL>
    <SQL action="update"><text>schterm_edit</text></SQL>
    <SQL action="delete"><text>schterm_del</text></SQL>
    <fields>
        <field name="schtermid" type="integer" header="ID" pk="true" format="0"/>
        <field name="aterminalid" type="integer" hidden="true"/>
        <field name="priority" type="integer" header="Приоритет" format="0"/>
        <field name="isactive" type="boolean" header="Активно" width="80"/>
        <field name="scheduleid" type="integer" header="Расписание" width="280"/>
    </fields>
</cursor>
