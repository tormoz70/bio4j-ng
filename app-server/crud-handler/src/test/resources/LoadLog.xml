<?xml version="1.0" encoding="UTF-8"?>
<store xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../res/schemas/store.xsd"
       maxExpRows="100000" multiselection="false">
    <SQL action="select" wrap="filter, sort">
        <text><![CDATA[{text-file:test1.sql}]]></text>
        <param type="integer" name="force_org_id"/>
        <param type="date" name="reg_from"/>
        <param type="date" name="reg_to"/>
        <param type="string" name="org_id"/>
        <param type="string" name="sess_prnt_org_id"/>
        <param type="string" name="sess_org_id"/>
        <param type="string" name="packet_name"/>
        <param type="string" name="ip"/>
        <param type="string" name="cur_pstate"/>
        <param type="string" name="message"/>
        <param type="string" name="load_method"/>
        <param type="string" name="test"/>
    </SQL>
	<fields>
		<field name="id" type="integer" hidden="true" pk="true"/>
		<field name="part_key" type="string" hidden="true"/>
        <field name="date_incoming" type="date" title="Дата/Время\nрегистрации"/>
        <field name="org_id" type="integer" format="0" title="ID Отправителя"/>
        <field name="sess_prnt_org_id" type="integer" format="0" title="ID Киносети"/>
        <field name="sess_org_id" type="integer" format="0" title="ID Кинотеатра"/>
        <field name="time_zone" type="integer" format="0" title="Часовой пояс"/>
		<field name="packet_name" type="string" title="Имя файла"/>
        <field name="zip_name" type="string" title="Имя zip-пакета"/>
        <field name="ip" type="string" title="Адрес клиента"/>
        <field name="is_loaded" type="boolean" hidden="true"/>
        <field name="cur_pstate0" type="integer" hidden="true"/>
        <field name="cur_pstate" type="string" title="Состояние"/>
        <field name="last_pstate_msg" type="string" title="Последнее сообшение"/>
		<field name="date_processing" type="date" title="Дата/Время\nобработки"/>
        <field name="is_log_downloaded" type="boolean" title="Ответ получен"/>
        <field name="log_downloaded" type="date" title="Дата/Время\nполучения ответа"/>
        <field name="vndname" type="string" title="Произв. ПО"/>
        <field name="vndcontacts" type="string" title="Произв. ПО\n(контакты)"/>
        <field name="load_method" type="string" title="Метод загрузки"/>
        <field name="test" type="string" title="Тестовый"/>
  </fields>
</store>
