<?xml version="1.0" encoding="UTF-8"?>
<cursor xmlns="http://bio4j.ru/biodef-repo/store"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://bio4j.ru/biodef-repo/store http://bio4j.ru/biodef-repo/store/store-v10.xsd"
        multiselection="true">
    <SQL action="select">
        <text><![CDATA[
SELECT
  aterminalid as sortord,
  aterminalid,
  aterminalkey,
  ip4addr,
  tstate,
  aname,
  anote,
  addr,
  latitude,
  longitude,
  isdeleted
FROM aterminal
WHERE not isdeleted
        ]]></text>
    </SQL>
 
    <fields>
        <field name="aterminalid" type="integer" header="ID" pk="true" format="0"/>
        <field name="aterminalkey" type="string" header="Уникальный ключ" align="left" width="40"/>

        <field name="ip4addr" type="string" header="IP адрес" align="left" width="80"/>
        <field name="tstate" type="string" header="Состояние" align="left" width="80"/>
        <field name="aname" type="string" header="Наименование" width="280"/>
        <field name="anote" type="string" header="Описание" width="280"/>
        <field name="addr" type="string" header="Расположение" width="150"/>
  </fields>
</cursor>
